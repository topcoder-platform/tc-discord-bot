{
    "name": "tc-discord-bot",
    "version": "1.0.0",
    "author": "Kiril Kartunov",
    "description": "A discord bot for Topcoder Community",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">=21.6.0"
    },
    "main": "dist/start.js",
    "scripts": {
        "lint": "tslint --project .",
        "lint:fix": "tslint --fix --project .",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "build": "tsc --project tsconfig.json",
        "register": "npm run build && node --enable-source-maps dist/start.js --register",
        "start": "node --enable-source-maps dist/start.js",
        "start:dev": "ts-node-dev --watch src/**/*.ts,config/**/*.json,lang/**/*.json src/start.ts",
        "start:shard": "node --enable-source-maps dist/app.js",
        "start:pm2": "npm run build && npm run pm2:start",
        "pm2:start": "pm2 start process.json",
        "pm2:stop": "pm2 stop process.json",
        "pm2:delete": "pm2 delete process.json"
    },
    "dependencies": {
        "@discordjs/rest": "2.2.0",
        "@sequelize/core": "^7.0.0-alpha.37",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.1",
        "contentful": "^10.8.8",
        "discord-api-types": "0.37.81",
        "discord.js": "13.5.0",
        "discord.js-rate-limiter": "1.3.2",
        "express": "4.19.2",
        "express-promise-router": "4.1.1",
        "filesize": "8.0.6",
        "jsonwebtoken": "^9.0.2",
        "jwks-rsa": "^3.1.0",
        "linguini": "1.3.1",
        "lodash": "^4.17.21",
        "node-fetch": "3.3.2",
        "node-schedule": "2.1.1",
        "one-liner-joke": "^1.2.2",
        "pg": "^8.11.5",
        "pino": "8.20.0",
        "pino-pretty": "11.0.0",
        "pm2": "^5.3.1",
        "qs": "^6.12.1",
        "reflect-metadata": "^0.2.2",
        "remove-markdown": "0.5.0",
        "tc-core-library-js": "^2.4.1"
    },
    "devDependencies": {
        "@types/express": "4.17.21",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/node": "20.12.7",
        "@types/node-fetch": "2.6.11",
        "@types/node-schedule": "2.1.7",
        "@types/remove-markdown": "0.3.4",
        "prettier": "^3.2.5",
        "ts-node-dev": "^2.0.0",
        "tslint": "^6.1.3",
        "typescript": "^4.5.4"
    }
}
